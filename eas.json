        "developmentClient": true
      }
    },
    "production": {
      "env": {
        "EXPO_USE_FORCE_LEGACY_DEPS": "true"
      },
      "channel": "production",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
